{
  "name": "t1",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "./node_modules/.bin/react-scripts start",
    "build": "./node_modules/.bin/react-scripts build && npm run build:locale",
    "test": "./node_modules/.bin/react-scripts test",
    "eject": "./node_modules/.bin/react-scripts eject",
    "lint": "./node_modules/.bin/eslint .",
    "----- // SSR CLEAN // -----": "",
    "clean:build:app": "rm -rf build",
    "clean:build:src": "rm -rf ./build-src",
    "clean:build:ssr": "rm -rf ./build-ssr",
    "clean:all": "npm run clean:build:app && npm run clean:build:src && npm run clean:build:ssr",
    "----- // SSR BUILD // -----": "",
    "build:locale": "NODE_ENV=development BABEL_ENV=node ./node_modules/.bin/babel src --out-dir ./build-locale && ./node_modules/.bin/babel react-intl-pack.js | node",
    "build:src": "NODE_ENV=production BABEL_ENV=node ./node_modules/.bin/babel src --out-dir ./build-src --copy-files",
    "build:ssr": "NODE_ENV=production BABEL_ENV=node ./node_modules/.bin/babel ssr --out-dir ./build-ssr",
    "build:node": "npm run build:src && npm run build:ssr",
    "build:all": "npm run build && npm run build:locale && npm run build:node",
    "----- // SSR START // -----": "",
    "start:dev": "./node_modules/.bin/nodemon index.dev.js",
    "start:prod": "npm run clean:all && npm run build:all && npm run start:prod:api",
    "start:dev:api": "./node_modules/.bin/nodemon --watch ssr --watch build -e js index.dev.js",
    "start:prod:api": "node index",
    "----- // STYLEGUIDE // -----": "",
    "styleguide": "BABEL_ENV=development ./node_modules/.bin/styleguidist server",
    "styleguide:build": "BABEL_ENV=development./node_modules/.bin/styleguidist build"
  },
  "proxy": "http://localhost:8080",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@marcopeg/hooks": "^0.2.2",
    "@marcopeg/react-ssr": "0.0.20",
    "@marcopeg/utils": "^1.12.0",
    "antd-mobile": "^2.2.8",
    "bcrypt-nodejs": "^0.0.3",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "es6-promise": "^4.2.5",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "express-locale": "^1.0.5",
    "graphql": "^14.0.2",
    "graphql-iso-date": "^3.6.1",
    "graphql-type-json": "^0.2.1",
    "helmet": "^3.15.0",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.2.0",
    "jsonwebtoken": "^8.4.0",
    "millisecond": "^0.1.2",
    "node-env-file": "^0.1.8",
    "pg": "^7.7.1",
    "react": "16.7.0-alpha.2",
    "react-container-dimensions": "^1.4.1",
    "react-dom": "16.7.0-alpha.2",
    "react-helmet": "^5.2.0",
    "react-icons": "^3.2.2",
    "react-intl": "^2.7.2",
    "react-loadable": "^5.5.0",
    "react-loader-spinner": "^2.3.0",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts-rewired": "2.1.1",
    "react-styleguidist": "^8.0.6",
    "sequelize": "^4.42.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-plugin-dynamic-import-node-babel-7": "^2.0.7",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-react-intl": "^3.0.1",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "eslint-config-react": "^1.1.7",
    "nodemon": "^1.18.9"
  }
}
